<!-- View all recipe types -->
<head>
  <h1><center>
    Recipe Types
  </center></h1>
</head>
<body>
  <ul>
    <% RecipeType.all.each do |type| %>
      <li>
        <b><%= type.id %></b> - <i><%= type.name %></i>
      </li>
    <% end %>
  </ul>
  <p>
    <a href="/add_recipe_type">Add new recipe type</a>
  </p>
</body>
<footer>
  <center>
    <button><a href="/home">Home</a></button>
  </center>
</footer>
